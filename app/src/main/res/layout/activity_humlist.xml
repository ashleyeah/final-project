<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#13294B"
    tools:context=".HumList">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView18"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Humanities and the Arts:"
            android:textColor="#E84A27"
            android:textSize="20sp"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/cwl207"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="CWL 207: Indian Cinema in Context"
            android:textColor="#E84A27" />

        <EditText
            android:id="@+id/faa220"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="FAA 220: Introduction to Fashion"
            android:textColor="#E84A27" />

        <EditText
            android:id="@+id/art105"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="ART 105: Visual Design for Non-Majors"
            android:textColor="#E84A27" />

        <EditText
            android:id="@+id/sped117"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="SPED 117: The Culture of Disability"
            android:textColor="#E84A27" />

        <EditText
            android:id="@+id/danc100"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="DANC 100: Intro to Contemporary Dance"
            android:textColor="#E84A27" />

        <EditText
            android:id="@+id/educ202"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="EDUC 202: Social Justice Sch and Society"
            android:textColor="#E84A27" />

        <EditText
            android:id="@+id/thea110"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="THEA 110: Broadway Musicals"
            android:textColor="#E84A27" />

        <EditText
            android:id="@+id/ece316"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="ECE 316: Ethics and Engineering"
            android:textColor="#E84A27" />

        <EditText
            android:id="@+id/art310"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="ART 310: Design Thinking"
            android:textColor="#E84A27" />

        <EditText
            android:id="@+id/fr156"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="FR 156: Exploring Paris"
            android:textColor="#E84A27" />

        <TextView
            android:id="@+id/credit2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="bottom"
            android:text="*Selection are based off http://waf.cs.illinois.edu/discovery/every_gen_ed_at_uiuc_by_gpa/"
            android:textColor="#E84A27"
            app:fontFamily="sans-serif-condensed-light" />
    </LinearLayout>
</LinearLayout>