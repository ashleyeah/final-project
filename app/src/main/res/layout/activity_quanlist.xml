<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".QuanList">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#0000FF"
        android:orientation="vertical">

        <EditText
            android:id="@+id/chlh421"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="CHLH 421: Health Data Analysis"
            android:textColor="#FF9800" />

        <EditText
            android:id="@+id/atms120"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="ATMS 120: Severe and Hazardous Weather"
            android:textColor="#FF9800" />

        <EditText
            android:id="@+id/kin401"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="KIN 401: Measure and Evaluate in Kinesiology"
            android:textColor="#FF9800" />

        <EditText
            android:id="@+id/cs105"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="CS 105: Intro Computing: Non-Tech"
            android:textColor="#FF9800" />

        <EditText
            android:id="@+id/stat212"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="STAT 212: Biostatistics"
            android:textColor="#FF9800" />

        <EditText
            android:id="@+id/stat200"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="STAT 200: Statistical Analysis"
            android:textColor="#FF9800" />

        <EditText
            android:id="@+id/cs101"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="CS 101: Intro Computing: Engineering and Science"
            android:textColor="#FF9800" />

        <EditText
            android:id="@+id/cs125"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="CS 125: Intro to Computer Science"
            android:textColor="#FF9800" />

        <EditText
            android:id="@+id/info102"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="INFO 102: Little Bits to Big Ideas"
            android:textColor="#FF9800" />

        <EditText
            android:id="@+id/soc380"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="SOC 380: Social Research Methods"
            android:textColor="#FF9800" />

        <TextView
            android:id="@+id/credit5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="bottom"
            android:text="*Selection are based off http://waf.cs.illinois.edu/discovery/every_gen_ed_at_uiuc_by_gpa/"
            android:textColor="#FF9800"
            app:fontFamily="sans-serif-condensed-light" />
    </LinearLayout>
</LinearLayout>